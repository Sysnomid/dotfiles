{
  "coc.preferences.formatOnSaveFiletypes": [
    "typescriptreact",
    "typescript",
    "json",
    "sh",
    "javascript",
    "javascriptreact",
    "css",
    "html",
    "vim",
    "prisma",
    "graphql"
  ],
  "languageserver": {
    "sql": {
      "command": "sql-language-server",
      "args": ["up", "--method", "stdio"],
      "filetypes": ["sql", "mysql"]
    }
  }
}
