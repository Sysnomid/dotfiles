{
  "coc.preferences.formatOnSaveFiletypes": [
    "typescriptreact",
    "typescript",
    "json",
    "sh",
    "javascript",
    "javascriptreact",
    "css",
    "html",
    "prisma",
    "graphql"
  ],
  "pairs.disableLanguages": ["vim"],
  "languageserver": {
    "sql": {
      "command": "sql-language-server",
      "args": ["up", "--method", "stdio"],
      "filetypes": ["sql", "mysql"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "clangd.path": "/home/svm/.config/coc/extensions/coc-clangd-data/install/12.0.0/clangd_12.0.0/bin/clangd"
}
